<!DOCTYPE mapper  
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.tuandai.ms.portal.dao.BuyerInfoDao">
  	<insert id="insertBuyerInfo" useGeneratedKeys="true" keyProperty="id">
  		insert into buyerinfo(name,address,telephone,productId) values(#{name},#{address},#{telephone},#{productId})
  	</insert>
</mapper>
